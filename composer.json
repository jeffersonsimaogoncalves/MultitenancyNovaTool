{
    "name": "jeffersonsimaogoncalves/multitenancy-nova-tool",
    "description": "A Laravel Nova tool to manage multitenancy.",
    "keywords": [
        "laravel",
        "nova",
        "multitenancy"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "jeffersonsimaogoncalves/multitenancy": "^3.0",
        "jeffersonsimaogoncalves/nova-permission": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "JeffersonSimaoGoncalves\\MultitenancyNovaTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeffersonSimaoGoncalves\\MultitenancyNovaTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
